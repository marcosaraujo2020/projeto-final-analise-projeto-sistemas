@model IEnumerable<Models.PagamentoCheque>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Banco)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NomeBanco)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NomeDoCobrado)
