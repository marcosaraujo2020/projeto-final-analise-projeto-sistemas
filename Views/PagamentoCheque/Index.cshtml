                @Html.DisplayFor(modelItem => item.Banco)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NomeBanco)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NomeDoCobrado)
            </td>
            <td>
